<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <template id="footer_default_extend" inherit_id="website.footer_default" customize_show="True" name="Footer Extend">
             <xpath expr="//div[@id='footer']//div[@id='info']//h2" position="replace">
                <h2>
                    <a target="_blanck" t-att-href="website.social_facebook" t-if="website.social_facebook"><i class="fa fa-facebook-square"/></a>
                    <a target="_blanck" t-att-href="website.social_twitter" t-if="website.social_twitter"><i class="fa fa-twitter"/></a>
                    <a target="_blanck" t-att-href="website.social_instagram" t-if="website.social_instagram" rel="publisher"><i class="fa fa-instagram"/></a>
                    <a target="_blanck" t-att-href="website.social_youtube" t-if="website.social_youtube"><i class="fa fa-youtube-play"/></a>
                    <a target="_blanck" t-att-href="website.social_linkedin" t-if="website.social_linkedin"><i class="fa fa-linkedin"/></a>
                    <a target="_blanck" t-att-href="website.social_pinterest" t-if="website.social_pinterest"><i class="fa fa-pinterest-square"/></a>
                    <a target="_blanck" t-att-href="website.social_whatsapp" t-if="website.social_whatsapp"><i class="fa fa-whatsapp"/></a>
                </h2>
            </xpath>
        </template>
    </data>
</openerp>
